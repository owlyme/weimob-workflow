    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "less": "^4.1.1",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-scripts": "^4.0.3",
